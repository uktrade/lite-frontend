[run]
omit =
  .venv/*
  conf/*.py
  manage.py
  **/tests/*
  *__init__*
  */admin.py
  */migrations/*
  **/conftest.py
  ui_tests/*
  unit_tests/*
  tests_common/*
  pipeline/*
  lite_content/*
  caseworker/search/proof_of_concept/*

[report]
exclude_lines =
  pragma: no cover
  raise NotImplementedError
  if settings.DEBUG
  if __name__ == "__main__"
